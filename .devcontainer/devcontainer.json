{
  "name": "PHP & MariaDB",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [8080, 3306],
  "containerEnv": {
    "MYSQL_ROOT_PASSWORD": "mariadb",
    "MYSQL_DATABASE": "mariadb",
    "MYSQL_USER": "mariadb",
    "MYSQL_PASSWORD": "mariadb"
  }
}
